#: forms.py:34
#, python-format
msgid ""
"%(ext)s is an invalid file extension. Authorized extensions are : "
"%(valid_exts_list)s"
msgstr ""
"%(ext)s es una extensión de fichero inválida. Las extensiones autorizadas son: "
"%(valid_exts_list)s"

#: forms.py:38
#, python-format
msgid ""
"Your file is too big (%(size)s), the maximum allowed size is "
"%(max_valid_size)s"
msgstr ""
"El fichero es demasiado grande (%(size)s), el tamaño máximo permitido es "
"%(max_valid_size)s"

#: forms.py:44
#, python-format
msgid ""
"You already have %(nb_avatars)d avatars, and the maximum allowed is "
"%(nb_max_avatars)d."
msgstr ""
"Usted actualmente tiene %(nb_avatars)d fotos de perfil, el máximo permitido es de "
"%(nb_max_avatars)d."


